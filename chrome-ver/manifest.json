{
	"manifest_version": 3,
	"name": "GoodSearch",
	"version": "2.2",
	"description": "Advance query searches, by using '/' at the start of the url search bar.",
	"icons": {
		"8": "icons/megu_8.jpeg",
		"16": "icons/megu_16.jpeg",
		"32": "icons/megu_32.jpeg",
		"48": "icons/megu_48.jpeg",
		"96": "icons/megu_96.jpeg",
		"128": "icons/megu_128.jpeg"
	},
	"permissions": ["search"],
	"omnibox": {
		"keyword": "/"
	},
	"background": {
		"service_worker": "background.js"
	}
}
